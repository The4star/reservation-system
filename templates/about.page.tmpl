{{template "base" .}}
{{define "content"}}
  <div class="container">
    <div class="row">
      <div class="col">
        <h1>About Page</h1>
        <p>{{index .StringMap "test"}}</p>
        <p>
          {{if ne (index .StringMap "remote_ip") ""}}
            Your IP address is {{index .StringMap "remote_ip"}}
          {{else}}
            I don't know your ip address yet. Visit the <a href="/">Home Page</a>
          {{end}}
        </p>
      </div>
    </div>
  </div>
  {{end}}
